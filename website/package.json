{
  "name": "eruizc-dev",
  "version": "0.2.5",
  "private": false,
  "description": "eruizc-dev",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "stop": "kill $(ps -aux | grep -e '.* node .*/gatsby develop$' | awk '{print $2}') && echo 'Development build stopped' || echo 'Development build not runing'",
    "prebuild": "npm run preload-fonts",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "firebase deploy",
    "preview": "firebase hosting:channel:deploy `git rev-parse --abbrev-ref HEAD`",
    "preload-fonts": "npm run start & (wait-on http://localhost:8000 && gatsby-preload-fonts && npm run stop)"
  },
  "license": "BSD 3 Clause",
  "dependencies": {
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^3.6.1",
    "gatsby-plugin-manifest": "^2.9.1",
    "gatsby-plugin-offline": "^3.7.1",
    "gatsby-plugin-preload-fonts": "^1.7.0",
    "gatsby-plugin-react-helmet": "^3.7.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.14.4",
    "gatsby-plugin-styled-components": "^3.7.0",
    "gatsby-source-filesystem": "^2.8.1",
    "gatsby-transformer-remark": "^4.3.0",
    "gatsby-transformer-sharp": "^2.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-useinterval": "^1.0.2",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "firebase-tools": "^9.12.0",
    "react-is": "^17.0.1",
    "wait-on": "^5.2.1"
  }
}
